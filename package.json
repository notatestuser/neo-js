{
  "name": "@cityofzion/neo-js",
  "version": "0.6.0",
  "private": false,
  "description": "A package for running a node on the neo blockchain.",
  "main": "dist/node.js",
  "scripts": {
    "lint": "./node_modules/.bin/concurrently \"npm-run eslint ./{dist,examples,test} --ext .js\" \"npm-run flow-jsdoc -d ./dist -o ./dist_flow && npm-run flow check && rm -rf ./dist_flow && npm-run rimraf ./dist_flow\"",
    "integration": "./node_modules/.bin/mocha -b ./test/integration/**/*.js",
    "test": "./node_modules/.bin/mocha -b ./test/unit/**/*.js",
    "coverage": "./node_modules/.bin/nyc npm test",
    "build:docs": "./node_modules/.bin/documentation build dist/** -f html -o ./docs",
    "rebuild": "rm -rf ./node_modules && rm -f ./package-lock.json && npm install",
    "prepush": "echo 'running lint...' && npm run lint && echo 'running tests...' && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CityOfZion/neo-js.git"
  },
  "keywords": [
    "neo",
    "blockchain",
    "cityofzion"
  ],
  "author": "lllwvlvwlll",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CityOfZion/neo-js/issues"
  },
  "homepage": "https://github.com/CityOfZion/neo-js#readme",
  "dependencies": {
    "@cityofzion/neon-js": "^2.1.1",
    "async": "~2.5.0",
    "axios": "^0.16.2",
    "lodash": "~4.17.4",
    "mongoose": "~4.11.3"
  },
  "devDependencies": {
    "axios-mock-adapter": "^1.9.0",
    "chai": "^4.1.2",
    "concurrently": "^3.5.1",
    "documentation": "^5.3.5",
    "eslint": "^4.9.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-flow": "^1.0.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.0",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "flow-bin": "^0.63.1",
    "flow-jsdoc": "^0.3.1",
    "husky": "^0.14.3",
    "mocha": "^3.5.3",
    "npm-run": "^4.1.2",
    "nyc": "^11.2.1",
    "rimraf": "^2.6.2",
    "sinon": "^4.1.3"
  }
}
